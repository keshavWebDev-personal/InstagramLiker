{
    "manifest_version": 3,
    "name": "InstagramLiker",
    "version": "0.1",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "128": "icon/icon128.png"
        }
    },
    "icons": {
        "128": "icon/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.instagram.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}